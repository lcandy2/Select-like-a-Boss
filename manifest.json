{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2

	, "name": "Select like a Boss"
	, "version": "2015.2.25"
	, "description": "Select link's text just like a regular text (like in Opera'12 browser) - Select like a Boss ;)"
	, "author": "Dzianis Rusak (dzian`is)"
	, "icons": {
		  "16": "icons/O16.png"
		, "48": "icons/O48.png"
		,"128": "icons/O128.png"
	}
	, "offline_enabled": true
	, "options_page": "help.html"
	, "content_scripts": [
		{
			  "matches": [ "\u003Call_urls>" ]
			, "all_frames": true
			, "match_about_blank": true
			, "run_at": "document_start"
			, "js": [ "includer.js" ]
		}
	]
	, "web_accessible_resources": [
		"script.js"
	]
	, "background": {
		  "scripts": ["eventPage.js"]
		, "persistent": false
	}
	, "page_action": {
		  "default_icon": "icons/O48.png"
		, "default_title": "Select like a Boss - Help page"
	}
	, "permissions": [
		  "tabs"
		, "storage"
		, "\u003Call_urls>"
	]
	, "key": ""
}
